<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.loscombos.blasco.myapplication2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity2"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".productos_leche"
            android:parentActivityName=".MainActivity2" />
        <activity
            android:name=".productos_cremoso"
            android:parentActivityName=".MainActivity2" />
        <activity
            android:name=".productos_desnatado"
            android:parentActivityName=".MainActivity2" />
        <activity
            android:name=".producto_trozos"
            android:parentActivityName=".MainActivity2" />
        <activity
            android:name=".productos_batido"
            android:parentActivityName=".MainActivity2" />
    </application>

</manifest>